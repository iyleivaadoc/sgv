@model web.Models.AsistenteTesoreria

@{
    ViewBag.Title = "Eliminar asistente";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="col-md-offset-3 col-md-6">
    <h4>¿Realmente desea eliminar este asistente de tesorería?</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Asistente")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Asistente.FullName)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color text-center">
            <input type="submit" value="Eliminar" class="btn btn-danger" /> 
            @Html.ActionLink("Cancelar", "Index", null, new { @class="btn btn-success"})
        </div>
    }
</div>
