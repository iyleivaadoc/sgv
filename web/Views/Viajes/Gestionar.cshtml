@model web.Models.Viajes
    @{
        ViewBag.Title = "Gestionar";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
@Html.Raw(Session["MyAlert"])
@{ Session["MyAlert"] = null; }
    
<div class="row" style="text-align:center;">
    <h2>Gestionar Proceso</h2>
    <div class="col-md-3"></div>
    <div class="col-md-6">
        <div class="col-md-5 well">
            <a href="~/Anticipos/Index?idviaje=@Model.IdViaje">
                <img src="~/Content/anticipo.png" style="max-width: 88%;" />
                ANTICIPO
            </a>
        </div>
        <div class="col-md-2 "></div>
        <div class="col-md-5 well">
            <a href="~/LiquidacionesViajes/Index?idviaje=@Model.IdViaje">
                <img src="~/Content/Liquidacion.png" style="max-width: 100%;" />
                LIQUIDACIÓN
            </a>
        </div>
    </div>
    <div style="position:absolute; bottom:1em;">
        @Html.ActionLink(" ","Index",null, new { @class = "btn-lg glyphicon glyphicon-arrow-left", @title = "Atrás" })
    </div>
</div>
